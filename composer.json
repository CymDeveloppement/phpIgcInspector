{
    "name": "ycdev/php-igc-inspector",
    "description": "Php library for manipulating IGC files",
    "type": "library",
    "license": "GPL-3.0-only",
    "keywords": [
        "igc",
        "gliding",
        "flight",
        "parser",
        "gps",
        "aviation",
        "soaring"
    ],
    "homepage": "https://github.com/CymDeveloppement/phpIgcInspector",
    "support": {
        "issues": "https://github.com/CymDeveloppement/phpIgcInspector/issues",
        "source": "https://github.com/CymDeveloppement/phpIgcInspector"
    },
    "autoload": {
        "psr-4": {
            "Ycdev\\PhpIgcInspector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ycdev\\PhpIgcInspector\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Yann",
            "email": "yann@cymdev.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
